{
    "updateToc": true,
    "showJSDocTags": false,
    "files": [
        "**.md",
        "site/styles/theme.css"
    ],
    "exclude": [
        ".git",
        "node_modules",
        "lib",
        "site/components/**",
        "site/styles/!(theme.css)",
        "site/pages/normal/normal.ts",
        "site/pages/landing/landing.ts",
        "src/extras/**",
        "src/tests/**",
        "README.md"
    ],
    "nodeModules": [
        {
            "path": "./src/tests/test-backend.ts",
            "outDir": "./backend",
            "backend": true
        }
    ],
    "deployMode": "dev",
    "frontMatter": {
        "projectName": "LiTScript",
        "repository": "https://github.com/johtela/litscript",
        "download": "https://www.npmjs.com/package/litscript",
        "license": "license.html",
        "logo": "images/bulb.svg",
        "footer": "Copyright Â© 2024 Tommi Johtela"
    }
}